double mul_matrix(int rowOne, int colOne, int rowTwo, int colTwo, double matrixOne[rowOne][colOne], double matrixTwo[rowTwo][colTwo]
                    );
#define TAMANHO 2

/*https://www.geeksforgeeks.org/c-program-multiply-two-matrices/*/
int main()
{
    double matrix1[TAMANHO][TAMANHO],matrix2[TAMANHO][TAMANHO];
    int i,j;
    printf("Preencha a primeira matriz:\n");
    for (i=0;i<TAMANHO;i++){
        for(j=0;j<TAMANHO;i++){
            printf("Insira o elemento da linha %d coluna %d",i+1,j+1);
            scanf("%f", matrix1[i][j]);
        }
    }
    printf("Preencha a segunda matriz:\n");
    for (i=0;i<TAMANHO;i++){
        for(j=0;j<TAMANHO;i++){
            printf("Insira o elemento da linha %d coluna %d",i+1,j+1);
            scanf("%f", matrix2[i][j]);
        }
    }
    mult_matrix(TAMANHO,TAMANHO,TAMANHO,TAMANHO,matrix1,matrix2);
    
}
