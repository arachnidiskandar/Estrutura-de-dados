void preencher_matrix(double *matrix);
double multi_matrix(int rowOne, int colOne, int rowTwo, int colTwo, double matrixOne[rowOne][colOne], double matrixTwo[rowTwo][colTwo]
                    );
int main()
{
    double matrix[3][3];
    int i,j;
    preencher_matrix(*matrix);
    for (i=0;i<3;i++){
        for(j=0;j<3;i++){
            printf("%f.1", matrix[i][j]);
        }
    }
}

void preencher_matrix(matrix){
    int row,col;
    for (row=0;row<3;row++){
        for(col=0;col<3;col++){
            /*printf("Insira o valor da linha %d coluna %d:",row,col)
            scanf("%f",  matrix[row][col]);*/
            matrix[row][col] += 1;
        }
    }
}
